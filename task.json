{
    "containerDefinitions": [ 
       { 
          
          "essential": true,
          "image": "599624599296.dkr.ecr.ap-south-1.amazonaws.com/sample:PRUDHVI-PROJECT-buildNumber",
          "logConfiguration": { 
             "logDriver": "awslogs",
             "options": { 
                "awslogs-group" : "/ecs/sample",
                "awslogs-region": "ap-south-1",
                "awslogs-stream-prefix": "ecs"
             }
          },
          "name": "httpd",
          "portMappings": [ 
             { 
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp"
             }
          ]
       }
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::599624599296:role/ecsTaskExecutionRole",
    "family": "sample",
    "memory": "1024",
    "requiresCompatibilities": [ 
        "FARGATE" 
     ]
 }
